@model Departments.ViewModel.DepartmentViewModel

<partial name="_DepartmentElement" model="@Model" />

@if (Model.SubDepartments != null && Model.SubDepartments.Any())
{
    <ul class="department-ul">
        @foreach (var subDepartment in Model.SubDepartments)
        {
            <partial name="_DepartmentPartial" model="@subDepartment" />
        }
    </ul>
}
